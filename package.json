{
  "name": "ornikar-test",
  "version": "0.1.0",
  "private": true,
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-pascal-case": "off"
    }
  },
  "dependencies": {
    "@ornikar/kitt": "27.1.0",
    "axios": "0.26.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.0",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@ornikar/browserslist-config": "3.3.1",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-native": "0.66.15",
    "cross-env": "7.0.3",
    "prettier": "2.5.1",
    "react-native-web": "0.17.5",
    "typescript": "4.5.5"
  },
  "scripts": {
    "start": "cross-env KITT_THEME_NAME=late-ocean react-scripts start",
    "build": "cross-env KITT_THEME_NAME=late-ocean react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "extends @ornikar/browserslist-config"
  ],
  "packageManager": "yarn@3.1.1"
}
