    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-picker/picker": "2.4.10",
    "@storybook/addons": "7.0.24",
    "axios": "1.4.0",
    "expo-linear-gradient": "12.1.2",
    "expo-web-browser": "12.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.4",
    "react-native": "0.72.0",
    "react-native-safe-area-context": "4.6.3",
